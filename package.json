  "scripts": {
    "dev": "npm run check:env && npm run dev:server",
    "dev:server": "cross-env NODE_ENV=development tsx server/index.ts",
    "dev:client": "vite",
    "dev:docker": "docker-compose up -d && npm run dev",
    "build": "npm run clean && npm run build:client && npm run build:server && npm run compress",
    "build:client": "vite build",
    "build:server": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist --minify --tree-shaking=true",
    "build:docker": "docker-compose build",
    "compress": "node scripts/compress.js",
    "clean": "rimraf dist",
    "start": "NODE_ENV=production node dist/index.js",
    "start:prod": "npm run build && npm start",
    "start:docker": "docker-compose up",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:reload": "pm2 reload ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "analyze": "npm run build:client -- --mode analyze",
    "optimize": "node scripts/optimize.js",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "env:init": "node scripts/generate-session-secret.mjs",
    "postinstall": "node scripts/generate-session-secret.mjs",
    "check:env": "node scripts/check-env.js",
    "generate:token-key": "npx tsx scripts/generate-token-key.ts",
    "test:email": "npx tsx scripts/test-send-email.ts",
    "generate:test-requirements": "npx tsx scripts/generate-test-requirements.ts",
    "generate:quick-requirements": "npx tsx scripts/quick-requirements-generator.ts",
    "generate:quick-consultants": "npx tsx scripts/quick-consultants-generator.ts",
    "security:validate": "npx tsx scripts/validate-security.ts",
    "docker:logs": "docker-compose logs -f",
    "docker:down": "docker-compose down",
    "docker:clean": "docker-compose down -v --remove-orphans"
  },
